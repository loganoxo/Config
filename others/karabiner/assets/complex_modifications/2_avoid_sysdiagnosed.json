{
    "title": "2. 屏蔽command+control+shift+option+w/点/逗号的功能(sysdiagnosed),统一改为vk_none(一个无任何功能的虚拟黑洞键)",
    "rules": [
        {
            "description": "2.1 屏蔽command+control+shift+option+w,改为vk_none",
            "manipulators": [
                {
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "vk_none"
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "2.2 屏蔽command+control+shift+option+/,改为vk_none",
            "manipulators": [
                {
                    "from": {
                        "key_code": "slash",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "vk_none"
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "2.3 屏蔽command+control+shift+option+点,改为vk_none",
            "manipulators": [
                {
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "vk_none"
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "2.4 屏蔽command+control+shift+option+逗号,改为vk_none",
            "manipulators": [
                {
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "vk_none"
                    },
                    "type": "basic"
                }
            ]
        }
    ]
}
