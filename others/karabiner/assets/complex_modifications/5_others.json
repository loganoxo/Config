{
    "title": "5. 其他功能性绑定",
    "rules": [
        {
            "description": "5.1 同时按下 a、s、d 三个按键 打开 Mission Control",
            "manipulators": [
                {
                    "from": {
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        },
                        "simultaneous": [
                            {
                                "key_code": "a"
                            },
                            {
                                "key_code": "s"
                            },
                            {
                                "key_code": "d"
                            }
                        ]
                    },
                    "to": [
                        {
                            "apple_vendor_keyboard_key_code": "mission_control"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "5.2 字母a => 一个无功能按键(f19),作为系统某些快捷键配置的填充",
            "manipulators": [
                {
                    "from": {
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        },
                        "key_code": "a"
                    },
                    "to": [
                        {
                            "key_code": "f19"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "5.2 字母b => f20",
            "manipulators": [
                {
                    "from": {
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        },
                        "key_code": "b"
                    },
                    "to": [
                        {
                            "key_code": "f20"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
