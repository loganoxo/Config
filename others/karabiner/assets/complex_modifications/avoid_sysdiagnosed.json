{
    "title": "1. 屏蔽command+control+shift+option+w/点/逗号的功能(sysdiagnosed)",
    "rules": [
        {
            "description": "1.1 屏蔽command+control+shift+option+w,改为f15",
            "manipulators": [
                {
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "f15"
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "1.2 屏蔽command+control+shift+option+点,改为f16",
            "manipulators": [
                {
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "f16"
                    },
                    "type": "basic"
                }
            ]
        },
        {
            "description": "1.3 屏蔽command+control+shift+option+逗号,改为f17",
            "manipulators": [
                {
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "control",
                                "option",
                                "shift"
                            ]
                        }
                    },
                    "to": {
                        "key_code": "f17"
                    },
                    "type": "basic"
                }
            ]
        }
    ]
}
